{
  "name": "client",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "start": "node ./build",
    "check": "svelte-check --tsconfig ./tsconfig.json --compiler-warnings 'a11y-no-redundant-roles:ignore'",
    "check:watch": "npm run check -- --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "test": "vitest run",
    "start-server": "cd .. && make serve",
    "test-e2e": "playwright test -c ./src/playwright.config.ts",
    "test-e2e:ci": "start-server-and-test start-server '3579/docs|3000' test-e2e",
    "test:watch": "vitest watch",
    "test:coverage": "npm run test -- --coverage"
  },
  "devDependencies": {
    "@playwright/test": "^1.18.1",
    "@sveltejs/adapter-node": "next",
    "@sveltejs/kit": "next",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/svelte": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "dotenv": "^16.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "isomorphic-fetch": "^3.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "start-server-and-test": "^1.14.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "vitest": "^0.2.6"
  },
  "type": "module",
  "dependencies": {
    "@gouvfr/dsfr": "^1.3.1",
    "date-fns": "^2.28.0",
    "svelte-forms-lib": "^2.0.1",
    "yup": "^0.32.11"
  }
}
